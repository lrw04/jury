{
    "base": "https://dl-cdn.alpinelinux.org/alpine/v3.17/releases/x86_64/alpine-minirootfs-3.17.3-x86_64.tar.gz",
    "run": [
        "apk add diffutils"
    ],
    "languages": {
        "c": {
            "install": [
                "apk add gcc"
            ],
            "filename": "solution.c",
            "compile": [
                "/usr/bin/gcc",
                "-Wall",
                "-std=c17",
                "-O3",
                "solution.c",
                "-o",
                "solution",
                "-lm"
            ],
            "run": [
                "./solution"
            ]
        },
        "cpp": {
            "install": [
                "apk add g++"
            ],
            "filename": "solution.cpp",
            "compile": [
                "/usr/bin/g++",
                "-Wall",
                "-std=c++17",
                "-O3",
                "solution.cpp",
                "-o",
                "solution",
                "-lm"
            ],
            "run": [
                "./solution"
            ]
        },
        "python": {
            "install": [
                "apk add python3"
            ],
            "filename": "solution.py",
            "compile": [
                "/usr/bin/python3",
                "-m",
                "py_compile",
                "solution.py"
            ],
            "run": [
                "/usr/bin/python3",
                "solution.py"
            ]
        },
        "java": {
            "install": [
                "apk add openjdk17-jdk"
            ],
            "filename": "Main.java",
            "compile": [
                "/usr/bin/javac",
                "Main.java"
            ],
            "run": [
                "/usr/bin/java",
                "Main"
            ]
        },
        "cl": {
            "install": [
                "apk add sbcl"
            ],
            "filename": "solution.lisp",
            "run": [
                "/usr/bin/sbcl",
                "--script",
                "solution.lisp"
            ]
        }
    }
}